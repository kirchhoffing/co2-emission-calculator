#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/co2-emission-calculator/node_modules/.pnpm/vite@7.0.1_@types+node@20.1_d75c2ba2fad3bcdd7019ba40a014861f/node_modules/vite/bin/node_modules:/mnt/d/co2-emission-calculator/node_modules/.pnpm/vite@7.0.1_@types+node@20.1_d75c2ba2fad3bcdd7019ba40a014861f/node_modules/vite/node_modules:/mnt/d/co2-emission-calculator/node_modules/.pnpm/vite@7.0.1_@types+node@20.1_d75c2ba2fad3bcdd7019ba40a014861f/node_modules:/mnt/d/co2-emission-calculator/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/co2-emission-calculator/node_modules/.pnpm/vite@7.0.1_@types+node@20.1_d75c2ba2fad3bcdd7019ba40a014861f/node_modules/vite/bin/node_modules:/mnt/d/co2-emission-calculator/node_modules/.pnpm/vite@7.0.1_@types+node@20.1_d75c2ba2fad3bcdd7019ba40a014861f/node_modules/vite/node_modules:/mnt/d/co2-emission-calculator/node_modules/.pnpm/vite@7.0.1_@types+node@20.1_d75c2ba2fad3bcdd7019ba40a014861f/node_modules:/mnt/d/co2-emission-calculator/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/vite@7.0.1_@types+node@20.1_d75c2ba2fad3bcdd7019ba40a014861f/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/vite@7.0.1_@types+node@20.1_d75c2ba2fad3bcdd7019ba40a014861f/node_modules/vite/bin/vite.js" "$@"
fi
